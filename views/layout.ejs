<!DOCTYPE html>
<html>
  <head>
    <title><%=typeof title == 'undefined' ? 'New Sails App' : title%></title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!--STYLES-->
    <link rel="stylesheet" href="/styles/importer.css">
    <!--STYLES END-->
  </head>

  <body ng-app="todoApp">
    <%- body %>


    <!--TEMPLATES-->
    <script type="text/javascript" src="/jst.js"></script>
    <!--TEMPLATES END-->


    <!--SCRIPTS-->
    <script src="/js/dependencies/sails.io.js"></script>
    <script src="/js/dependencies/dist/jquery.js"></script>
    <script src="/js/dependencies/angular.js"></script>
    <script src="/js/dependencies/angular-mocks.js"></script>
    <script src="/js/dependencies/angular-moment.js"></script>
    <script src="/js/dependencies/angular-route.js"></script>
    <script src="/js/dependencies/angular-sanitize.js"></script>
    <script src="/js/dependencies/assets/javascripts/bootstrap.js"></script>
    <script src="/js/dependencies/build/ng-lodash.js"></script>
    <script src="/js/dependencies/moment.js"></script>
    <script src="/js/dependencies/release/angular-ui-router.js"></script>
    <script src="/js/dependencies/ui-bootstrap-tpls.js"></script>
    <script src="/js/app/main.js"></script>
    <script src="/js/app/services/TodoService.js"></script>
    <!--SCRIPTS END-->

    <% if (sails.config.livereloadPath) { %>
        <script src="<%= sails.config.livereloadPath %>"></script>
    <% } %>
  </body>
</html>
